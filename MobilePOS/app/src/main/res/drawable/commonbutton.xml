<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <gradient android:startColor="@color/darkGreen" android:endColor="#ff8e111e" android:angle="270.0" />
            <corners android:radius="3.0dip" />
            <padding android:top="6.0dip" android:bottom="6.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/darkGreen" android:endColor="#ff8e111e" android:angle="270.0" />
            <corners android:radius="3.0dip" />
            <padding android:top="6.0dip" android:bottom="6.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <gradient android:startColor="#fff5a9b0" android:endColor="#fff5a9b0" android:angle="270.0" />
            <corners android:radius="3.0dip" />
            <padding android:top="6.0dip" android:bottom="6.0dip" />
        </shape>
    </item>
</selector>
